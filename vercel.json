{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python@3.1.32",
        "config": {
          "runtime": "python3.9",
          "installCommand": "pip install --upgrade pip setuptools wheel && pip install -r requirements.txt",
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }